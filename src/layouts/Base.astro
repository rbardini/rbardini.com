---
import Footer from '../components/Footer.astro'
import Head from '../components/Head.astro'

export interface Props {
  title?: string
}

const { title } = Astro.props
---

<html lang="en">
  <head>
    <Head {title} />
  </head>
  <body>
    <main>
      <slot />
    </main>
    <Footer />
  </body>
</html>
